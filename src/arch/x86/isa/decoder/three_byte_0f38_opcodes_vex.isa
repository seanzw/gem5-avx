////////////////////////////////////////////////////////////////////
//
// Decode the three byte opcodes with the 0f38 prefix.
//
'X86ISA::ThreeByte0F38Opcode': decode LEGACY_OP {
    format Inst {
        1: decode LEGACY_DECODEVAL {
            // 0x66
            0x1: decode OPCODE_OP {
                0x18: decode EVEX_L {
                    0x1: VBROADCASTSS(VYdo, Wq);
                    0x0: VBROADCASTSS(Vo, Wq);
                }
                0x28: decode EVEX_L {
                    0x1: VPMULDQ(VYdo, WYdo);
                    0x0: VPMULDQ(Vo, Wo);
                }
                0x2B: decode EVEX_L {
                    0x1: VPACKUSDW(VYdo, WYdo);
                    0x0: VPACKUSDW(Vo, Wo);
                }
                0x30: decode EVEX_L {
                    0x1: VPMOVZXBW(VYdo, WYdo);
                    0x0: VPMOVZXBW(Vo, Wo);
                }
                0x39: decode EVEX_L {
                    0x1: VPMINSD(VYdo, WYdo);
                    0x0: VPMINSD(Vo, Wo);
                }
                0x40: decode EVEX_L {
                    0x1: VPMULLD(VYdo, WYdo);
                    0x0: VPMULLD(Vo, Wo);
                }
                0x58: decode EVEX_L {
                    0x1: VPBROADCASTD(VYdo, Wd);
                    0x0: VPBROADCASTD(Vo, Wd);
                }
                0x78: decode EVEX_L {
                    0x1: VPBROADCASTB(VYdo, Wb);
                }
                0x99: decode EVEX_B {
                    0x0: decode EVEX_L {
                        0x0: VFMADD132SS(Vo, Wq);
                    }
                }
                0xA9: decode EVEX_B {
                    0x0: decode EVEX_L {
                        0x0: VFMADD213SS(Vo, Wq);
                    }
                }
                0xAD: decode EVEX_B {
                    0x0: decode EVEX_L {
                        0x0: VFNMADD213SS(Vo, Wq);
                    }
                }
                0xB8: decode EVEX_B {
                    0x0: decode EVEX_L {
                        0x1: VFMADD231PS(VYdo, WYdo);
                        0x0: VFMADD231PS(Vo, Wo);
                    }
                }
                0xB9: decode EVEX_B {
                    0x0: decode EVEX_L {
                        0x0: VFMADD231SS(Vo, Wq);
                    }
                }
                0xBD: decode EVEX_B {
                    0x0: decode EVEX_L {
                        0x0: VFNMADD231SS(Vo, Wq);
                    }
                }
                0xF7: decode EVEX_L {
                    0x0: decode REX_W {
                        0x1: SHLXQ(Ev, Gv);
                    }
                }
            }
        }
    }
}
